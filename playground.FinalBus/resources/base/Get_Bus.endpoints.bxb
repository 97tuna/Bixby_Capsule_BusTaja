endpoints {
	action-endpoints {
		action-endpoint (SearchBus) {
			accepted-inputs (location, BusNumber)
			local-endpoint ("main/searchBus.js")
		}
		
		action-endpoint (SearchNearStation) {
			accepted-inputs (location, Near, StationTag)
			local-endpoint ("main/searchNearStation.js")
		}
		action-endpoint (OpenKakaoMap) {
			accepted-inputs (MapURL)
			local-endpoint ("KakaoMap/OpenKakaoMap.js") //지도 클릭시 브라우져 띄우기
		}
	}
}